<?xml version="1.0"?>
<robot xmlns:xacro="http://www.ros.org/wiki/xacro" name="FourWheeledRobot">

    <xacro:include filename="$(find robot_description)/urdf/material.xacro" />
    <xacro:include filename="$(find robot_description)/urdf/gazebo_plugins.gazebo" />

    <!-- This is the base link  -->
    <link name="base_link"/>

    <joint name="chassis_joint" type="fixed">
        <origin xyz="0.0 0.0 0.25" rpy="0.0 0.0 0.0"/>
        <parent link="base_link"/>
        <child link="chassis_link"/>
        <axis xyz="0.0 0.0 0.0"/>
    </joint>

    <!-- This is the chassis link  -->

    <link name="chassis_link">
        <inertial>
            <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0" />
            <mass value="1.0" />
            <inertia ixx="0.1" ixy="0.0" ixz="0.0" iyy="0.1" iyz="0.0" izz="0.1" />
        </inertial>
        <visual name="chassis_visual">
            <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0" />
            <geometry>
                <box size="0.5 0.3 0.1" />
            </geometry>
            <material name="Black">
                <color rgba="0 0 0 1" />
            </material>
        </visual>
        <collision name="chassis_collision">
            <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0" />
            <geometry>
                <box size="0.5 0.3 0.15" />
            </geometry>
        </collision>
    </link>
    <gazebo reference="chassis_link">
        <material name="Black">
            <color rgba="0 0 0 1" />
        </material>
    </gazebo>
    <!-- Back right wheel -->
    <joint name="wheel1_joint" type="continuous">
        <origin xyz="-0.15 -0.177 -0.05" rpy="0.0 0.0 0.0" />
        <parent link="chassis_link" />
        <child link="wheel1_link" />
        <axis xyz="0.0 1.0 0.0" />
        <limit lower="0.0" upper="0.0" effort="0.0" velocity="0.0" />
    </joint>
    <link name="wheel1_link">
        <inertial>
            <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0" />
            <mass value="0.5" />
            <inertia ixx="0.1" ixy="0.0" ixz="0.0" iyy="0.1" iyz="0.0" izz="0.1" />
        </inertial>
        <visual name="wheel1_visual">
            <origin xyz="0.0 0.0 0.0" rpy="1.570795 0.0 0.0" />
            <geometry>
                <cylinder length="0.05" radius="0.075" />
            </geometry>
            <material name="red">
                <color rgba="1 0 0 1" />
            </material>
        </visual>
        <collision name="wheel1_collision">
            <origin xyz="0.0 0.0 0.0" rpy="1.570795 0.0 0.0" />
            <geometry>
                <cylinder length="0.05" radius="0.075" />
            </geometry>
        </collision>
    </link>
    <gazebo reference="wheel1_link">
        <material name="red">
            <color rgba="1 0 0 1" />
        </material>
    </gazebo>
    <!-- Back left wheel -->
    <joint name="wheel2_joint" type="continuous">
        <origin xyz="-0.15 0.177 -0.05" rpy="0.0 0.0 0.0" />
        <parent link="chassis_link" />
        <child link="wheel2_link" />
        <axis xyz="0.0 1.0 0.0" />
        <limit lower="0.0" upper="0.0" effort="0.0" velocity="0.0" />
    </joint>
    <link name="wheel2_link">
        <inertial>
            <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0" />
            <mass value="0.5" />
            <inertia ixx="0.1" ixy="0.0" ixz="0.0" iyy="0.1" iyz="0.0" izz="0.1" />
        </inertial>
        <visual name="wheel2_visual">
            <origin xyz="0.0 0.0 0.0" rpy="1.570795 0.0 0.0" />
            <geometry>
                <cylinder length="0.05" radius="0.075" />
            </geometry>
            <material name="green">
                <color rgba="0 1 0 1" />
                <texture filename="" />
            </material>
        </visual>
        <collision name="wheel2_collision">
            <origin xyz="0.0 0.0 0.0" rpy="1.570795 0.0 0.0" />
            <geometry>
                <cylinder length="0.05" radius="0.075" />
            </geometry>
        </collision>
    </link>
    <gazebo reference="wheel2_link">
        <material name="green">
            <color rgba="0 1 0 1" />
        </material>
    </gazebo>
    <!-- front right wheel -->
    <joint name="wheel3_joint" type="continuous">
        <origin xyz="0.15 -0.177 -0.05" rpy="0.0 0.0 0.0" />
        <parent link="chassis_link" />
        <child link="wheel3_link" />
        <axis xyz="0.0 1.0 0.0" />
        <limit lower="0.0" upper="0.0" effort="0.0" velocity="0.0" />
    </joint>
    <link name="wheel3_link">
        <inertial>
            <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0" />
            <mass value="0.5" />
            <inertia ixx="0.1" ixy="0.0" ixz="0.0" iyy="0.1" iyz="0.0" izz="0.1" />
        </inertial>
        <visual name="wheel3_visual">
            <origin xyz="0.0 0.0 0.0" rpy="1.570795 0.0 0.0" />
            <geometry>
                <cylinder length="0.05" radius="0.075" />
            </geometry>
            <material name="green">
                <color rgba="0 1 0 1" />
                <texture filename="" />
            </material>
        </visual>
        <collision name="wheel3_collision">
            <origin xyz="0.0 0.0 0.0" rpy="1.570795 0.0 0.0" />
            <geometry>
                <cylinder length="0.05" radius="0.075" />
            </geometry>
        </collision>
    </link>
    <gazebo reference="wheel3_link">
        <material name="green">
            <color rgba="0 1 0 1" />
        </material>
    </gazebo>
    <!-- front left wheel -->
    <joint name="wheel4_joint" type="continuous">
        <origin xyz="0.15 0.177 -0.05" rpy="0.0 0.0 0.0" />
        <parent link="chassis_link" />
        <child link="wheel4_link" />
        <axis xyz="0.0 1.0 0.0" />
        <limit lower="0.0" upper="0.0" effort="0.0" velocity="0.0" />
    </joint>
    <link name="wheel4_link">
        <inertial>
            <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0" />
            <mass value="0.5" />
            <inertia ixx="0.1" ixy="0.0" ixz="0.0" iyy="0.1" iyz="0.0" izz="0.1" />
        </inertial>
        <visual name="wheel4_visual">
            <origin xyz="0.0 0.0 0.0" rpy="1.570795 0.0 0.0" />
            <geometry>
                <cylinder length="0.05" radius="0.075" />
            </geometry>
            <material name="red">
                <color rgba="1 0 0 1" />
                <texture filename="" />
            </material>
        </visual>
        <collision name="wheel4_collision">
            <origin xyz="0.0 0.0 0.0" rpy="1.570795 0.0 0.0" />
            <geometry>
                <cylinder length="0.05" radius="0.075" />
            </geometry>
        </collision>
    </link>
    <gazebo reference="wheel4_link">
        <material name="red">
            <color rgba="1 0 0 1" />
        </material>
    </gazebo>
    
    <!-- LIDAR-->
    <link name="lider_link">
        <inertial>
            <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0" />
            <mass value="0.10" />
            <inertia ixx="1e-6" ixy="0.0" ixz="0.0" iyy="1e-6" iyz="0.0" izz="1e-6" />
        </inertial>
        <visual>
        <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>            <geometry>
                <cylinder length="0.1" radius="0.05"/>
                <mesh filename="package://robot_description/meshes/rplidar.dae" />
            </geometry>
         <material name="gold">
                <color rgba="1 0.843137 0 1" />
        </material>
        </visual>

        <collision>
            <origin xyz="0.0 0.0 0.1" rpy="0.0 0.0 0.0" />
            <geometry>
                <mesh filename="package://robot_description/meshes/rplidar.dae" />
            </geometry>
        </collision>
    </link>

    <joint name="lider_joint" type="fixed">
        <origin xyz="0.2 0.0 0.10" rpy="0 0 0" />
        <parent link="chassis_link" />
        <child link="lider_link" />
        <axis xyz="0 0 0" />
    </joint>




</robot>